{
  "indent_size": 2, // Indentation size
  "indent_char": " ", // Indentation character
  "indent_with_tabs": false,
  "end_with_newline": true,
  "preserve_newlines": true, // Whether existing line breaks before elements should be preserved (only works before elements, not inside tags or for text)
  "max_preserve_newlines": 10, // Maximum number of line breaks to be preserved in one chunk (0 disables)

  "css": {
    "selector_separator_newline": false, // Separate selectors with newline or not (e.g. "a,\nbr" or "a, br")
    "newline_between_rules": true, // Add a new line after every css rule
    "space_around_combinator": true
  },

  "html": {
    "indent_inner_html": false, // Indent <head> and <body> sections
    "brace_style": "collapse,preserve-inline", // [collapse|expand|end-expand|none] Put braces on the same line as control statements (default), or put braces on own line (Allman / ANSI style), or just put end braces on own line, or attempt to keep them where they are
    "indent_scripts": "keep", // [keep|separate|normal]
    "wrap_line_length": 0, // Lines should wrap at next opportunity after this number of characters (0 disables)
    "wrap_attributes": "force",
    "wrap_attributes_indent_size": 4,
    "unformatted": ["a", "meta", "script", "link", "span", "img", "code", "pre", "sub", "sup", "em", "strong", "b", "i", "u", "strike", "big", "small", "pre", "h1", "h2", "h3", "h4", "h5", "h6"], // List of tags that should not be reformatted
    "content_unformatted": ["pre"],
    "extra_liners": ["head", "body", "/html"]
  },

  "js": {
    "indent_level": 0, // Initial indentation level
    "space_in_paren": false, // Add padding spaces within paren, ie. f( a, b )
    "space-in-empty-paren": false, //Add a single space inside empty paren, ie. f( )
    "jslint_happy": false, // If true, then jslint-stricter mode is enforced
    "space_after_anon_function": true, // Should the space before an anonymous function's parens be added, "function()" vs "function ()"
    "brace_style": "collapse,preserve-inline", // [collapse|expand|end-expand|none] Put braces on the same line as control statements (default), or put braces on own line (Allman / ANSI style), or just put end braces on own line, or attempt to keep them where they are
    "break_chained_methods": false, // Break chained method calls across subsequent lines
    "keep_array_indentation": true, // Preserve array indentation
    "keep_function_indentation": false, // Preserve function indentation
    "unescape_strings": false, // Should printable characters in strings encoded in \xNN notation be unescaped, "example" vs "\x65\x78\x61\x6d\x70\x6c\x65"
    "wrap_line_length": 0, // Lines should wrap at next opportunity after this number of characters (0 disables)
    "e4x": false, // Pass E4X xml literals through untouched
    "comma_first": false,
    "space_before_conditional": true // Should the space before conditional statement be added, "if(true)" vs "if (true)"
  }
}
